{
	"name": "theme-ocean",
	"displayName": "Ocean Theme",
	"description": "üé® an ocean style theme of vscode",
	"version": "0.4.6",
	"publisher": "alanlang",
	"license": "MIT",
	"homepage": "https://github.com/AlanLang/vscode-theme-ocean",
	"repository": {
		"type": "git",
		"url": "https://github.com/AlanLang/vscode-theme-ocean"
	},
	"scripts": {
		"vsce-publish": "echo please run 'vsce publish'",
		"build": "vsce package"
	},
	"engines": {
		"vscode": "^1.12.0"
	},
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension",
	"icon": "icon.png",
	"contributes": {
		"themes": [
			{
				"label": "Ocean",
				"uiTheme": "vs-dark",
				"path": "./themes/ocean.json"
			}
		],
		"commands": [
			{
				"command": "extension.insertLogStatement",
				"title": "Insert Log Statement"
			},
			{
				"command": "extension.deleteAllLogStatements",
				"title": "Delete all console.log statements"
			},
			{
				"command": "extension.switchFile",
				"title": "switch file ÂàáÊç¢Êñá‰ª∂"
			},
			{
				"command": "extension.listTodo",
				"title": "list todo ÂàóÂá∫ÂæÖÂäû‰∫ãÈ°π"
			},
			{
				"command": "extension.commitizen",
				"title": "commitizen commit ‰ª£Á†ÅÊèê‰∫§"
			}
		],
		"keybindings": [
			{
				"command": "extension.insertLogStatement",
				"key": "shift+ctrl+l",
				"mac": "shift+cmd+l",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.deleteAllLogStatements",
				"key": "shift+ctrl+d",
				"mac": "shift+cmd+d"
			},
			{
				"command": "extension.switchFile",
				"key": "shift+ctrl+w",
				"mac": "shift+cmd+w"
			}
		]
	},
	"configuration":{
		"type": "object",
		"title": "ocean theme configuration",
		"proerties": {
			"commitizen.hasEmoji": {
				"type": "boolean",
				"default": false,
				"description": "has enmoji in commit message"
			},
			"commitizen.simple": {
				"type": "boolean",
				"default": true,
				"description": "simple git message"
			}
		}
	},
	"__metadata": {
		"id": "6cd30b49-437a-4779-92d1-126c305a9b0f",
		"publisherDisplayName": "Alan"
	},
	"dependencies": {
		"execa": "^1.0.0"
	}
}
