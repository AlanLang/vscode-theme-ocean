{
	"name": "theme-ocean",
	"displayName": "Ocean Theme",
	"description": "üé® an ocean style theme of vscode",
	"version": "0.2.2",
	"publisher": "alanlang",
	"license": "MIT",
	"homepage": "https://github.com/AlanLang/vscode-theme-ocean",
	"repository": {
		"type": "git",
		"url": "https://github.com/AlanLang/vscode-theme-ocean"
	},
	"scripts": {
		"vsce-publish": "echo please run 'vsce publish'",
		"build": "vsce package"
	},
	"engines": {
		"vscode": "^1.12.0"
	},
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"onCommand:extension.insertLogStatement",
		"onCommand:extension.deleteAllLogStatements",
		"onCommand:extension.switchFile"
	],
	"main": "./extension",
	"icon": "icon.png",
	"contributes": {
		"themes": [
			{
				"label": "Ocean",
				"uiTheme": "vs-dark",
				"path": "./themes/ocean.json"
			}
		],
		"commands": [
			{
					"command": "extension.insertLogStatement",
					"title": "Insert Log Statement"
			},
			{
					"command": "extension.deleteAllLogStatements",
					"title": "Delete all console.log statements"
			},
			{
        "command": "extension.switchFile",
				"title": "switch file ÂàáÊç¢Êñá‰ª∂"
     }
		],
		"keybindings": [
				{
						"command": "extension.insertLogStatement",
						"key": "shift+ctrl+l",
						"mac": "shift+cmd+l",
						"when": "editorTextFocus"
				},
				{
						"command": "extension.deleteAllLogStatements",
						"key": "shift+ctrl+d",
						"mac": "shift+cmd+d"
				},
				{
						"command": "extension.switchFile",
						"key": "shift+ctrl+w",
						"mac": "shift+cmd+w"
				}
		]
	},
	"__metadata": {
		"id": "6cd30b49-437a-4779-92d1-126c305a9b0f",
		"publisherDisplayName": "Alan"
	}
}